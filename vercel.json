{
  "rewrites": [
    {
      "source": "/api/cron",
      "destination": "/api/cron"
    },
    {
      "source": "/api/weather",
      "destination": "/api/index"
    },
    {
      "source": "/api/evaluation",
      "destination": "/api/index"
    },
    {
      "source": "/api/email-config",
      "destination": "/api/index"
    },
    {
      "source": "/api/test-email",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 5 * * *"
    }
  ]
}

